
SELECT VW.FUND_CODE as fundCode, VW.GROUP_CODE as groupCode,
VW.FUND_SOURCE as fundSource, VW.FUND_NAME as fundName, VW.GROUP_NAME as
groupName, VW.FUND_TYPE as fundType, REGION.DESCRIPTION_LOCAL as
regionLocalName, VW.ASSET_TYPE_LOCAL as assetTypeLocal, VW.ASSET_TYPE_EN
as assetTypeEn, VW.TYPE as type, VW.LV as lv, VW.FUND_IN_FOCUS as
fundInFocus, NAV.FUND_CURRENCY as fundCurrency, NAV.PRICE_DATE as
fundPriceDate, decode(upper(VW.FUND_TYPE), 'ONSHORE',
to_char(round(NAV.TODAY_NAV, 2),
'FM9999999999999990.00'),to_char(round(NAV.TODAY_NAV, 4),
'FM9999999999999990.0000')) as fundTodayNav,
to_char(round(NAV.PREV_DAY_CHANGE_PER * 100, 2),'FM9999999999999990.00')
as pervDayChangePer,
decode(upper(VW.FUND_TYPE),'ONSHORE',to_char(round(PRICE.IN_TAX_PRICE,
2), 'FM9999999999999990.00'),to_char(round(PRICE.IN_TAX_PRICE,
4),'FM9999999999999990.0000')) as resTexableNav,
decode(upper(VW.FUND_TYPE),'ONSHORE',to_char(round(PRICE.IN_ADV_TAX_PRICE,
2),'FM9999999999999990.00'),to_char(round(PRICE.IN_ADV_TAX_PRICE,
4),'FM9999999999999990.0000')) as nonResTaxableNav,
VW.INVESTMENT_REGION_CODE as investmentRegionCode FROM FM_FUND_VIEW
VW,FM_INV_REGION_LOCAL REGION, FM_NAV_RETURNS_LATEST
NAV,FM_FUND_PRICE_ONSHORE_LATEST PRICE WHERE
REGION.INVESTMENT_REGION_CODE = VW.INVESTMENT_REGION_CODE AND
REGION.LANG_CODE = '1042' AND VW.FUND_CODE = NAV.FUND_CODE AND
VW.FUND_SOURCE = NAV.FUND_SOURCE AND PRICE.FUND_CODE(+) = VW.FUND_CODE
AND PRICE.FUND_SOURCE(+) = VW.FUND_SOURCE AND VW.FUND_TYPE IN
('OnShore') AND REGION.INVESTMENT_REGION_CODE IN ('5','8') AND
VW.FUND_GRP_CODE IN ('BOF') ORDER BY
NLSSORT(VW.GROUP_NAME,'NLS_SORT=KOREAN_M'),groupCode,VW.LV,VW.SEQUENCE_ORDER 